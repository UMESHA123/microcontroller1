//WRITE AN ASSEMBLY LANGUAGE PROGRAM TO CONVERT THE HEX TO DECIMAL NUMBER



MOV A,#0FEH	 // MOVE THE  IMMEDIATE ADDRESS FEH TO THE ACCUMALETER A HENCES THE A=EFH
MOV B,#0AH // MOVE THE IMMEDIAT ADDRESS 0AH TO THE RESITER B HENCE THE B =0AH
DIV AB	// DEVIED THE A RESITER WITH THE B RESITER AND A STORES HEIGHTER NIBBEL AND B STORES LOWER NIBBEL
MOV R0,B // MOVE THE RESITER B CONTENT TO THE RESITER R0
MOV B,#0AH // MOVE THE IMMEDIAT ADDRESS TO THE RESITER B
DIV AB	 //DIVIED THE A BY B AND A STORES HIGTHER NIBBEL AND B STORES LOWER NIBBEL
MOV 30H,A  // MOVE THE A CONTENT TO THE 30H MEMORY LOCATION
MOV A,R0  // MOVE THE R0 CONTENT TO THE ACCUMALETER
SWAP A	 //SWAP THE A 
ORL A,R0	// A OR R0(OR OPARETION BETWEEN A RESITER WITH THE R0 RESITER)
MOV 31H,A  // MOVE THE ACCUMALETER CONTENT TO A TO THE 30H MEMORY LOCATION
END	   // END THE PROGRAM



//EXAMPLE
A=0FEH
B=0AH
DIVIED A/B =0FE/0A  THEN A=19 AND B=04
R0=04
B=0A
DIVIED A/B =19/0A  THEN A=02 B=0
30H=02
A=R0  RO=04 THEN A=0A
SWAP A THEN A=40
(A OR R0) THEN A=40 AND R0=04  THEN A=44 ( OR =IS AN  ADDITION OPARETION)`
31H= 44

