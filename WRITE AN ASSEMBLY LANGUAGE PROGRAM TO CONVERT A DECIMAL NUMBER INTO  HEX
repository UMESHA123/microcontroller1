//WRITE AN ASSEMBLY LANGUAGE PROGRAM TO CONVERT A DECIMAL NUMBER INTO  HEX




MOV A,#95H // MOVE THE IMMEDIAT ADDRESS 95H TO THE ACCUMALETER A
MOV B,#0AH // MOVE THE IMMEDIAT ADDTESS 0AH TO THE RESITER B
DIV AB	// DIVIED THE A RESITER WITH THE B RESITER AND HIEGHTER NIBBEL IS STORED IN THE A RESOER AND LOWER NIBBEL IS STORED IN THE B RESITER
MOV R0,B // MOVE THE CONTENT OF THE B RESITER TO THE RESITER R0
MOV B,#0AH	// MOVE THE IMMEDIAT ADDERESS TO THE RESITER B
MUL AB	 // MULTIFLY THE A RESITER WITH B RESITER THE HIGHER NIBBEL IS STORED IN THE A RESITER AND LOWER NIBBEL IS STORED IN THE B RESITER
ADD A,R0 // ADD THE CONTENT A WITH THE R0 CONTENT AND THE RESULT IS STORED IN THE ACCUMALETER A
MOV 30H,A // MOVE THE ACCUMALETER CONTENT TO THE 30H MEMORY LOCATION
END		  // END THE PROGRAM


 A=95
 B=0AH
 DIVIED A/B  =95H/0AH  A=0EH AND B=09H
 R0=09H
 B=0AH
 MULTIFLY A*B 0EH*0AH = A=8CH AND B=00H
 ADDITION A+R0 8CH + 09H =95 (DECIMAL) 
 30H = 95




